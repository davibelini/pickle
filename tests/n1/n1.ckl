(sec dta( // data section where you define data structures
  (fnc log // declare log function
    [1, ...] // log receives mandatorily 1 argument
      (? (rgs.len > 1) // if there is more than 1 argument
        (prt (each rgs))) // print each of the elements
      (:) // else
        (prt (rgs.1))) // print the first
  (var msg "Hello, World!"))) // declare msg variable(outside of the log function)

(sec cde( // code section where you use your structures
  (log msg)))