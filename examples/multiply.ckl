(inf INDMOD) // independent module(can be executed alone without an inf.ckl file)

(sec dta     // data section
  (set multiply
    (fnc   // function multiply
      [2]           // receives 2 arguments
        (rtn (rgs.1 * rgs.2)))  // return argument 1 * argument 2
    (set fct1 4)
    (set fct2 5)
    (set rst (multiply fct1 fct2))))

(sec cde
  (prt rst))